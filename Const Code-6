
public class Const; 
String name;
int age;
public Person(StringName, int Age) { 
    public void displayInfo() {
        System.out.println("Name: " + name);
        System.out.println("Age: " + age);
    }
    public static void main(String[] args) {
        Person p1 = new Person("Soham", 44);
        p1.displayInfo();
    }
}
