public class Bike {
    public void () {
        System.out.println("Bike makes a sound");
    }
}
class Honda extends Bike {
    
    public void () {
        System.out.println("Honda is company");
    }
}

class Hero extends Bike {
    public void () {
        System.out.println("Hero is company");
    }
}

public class Main {
    public static void main(String[] args) {
        Bike myHonda = new Honda(); 
        Bike myHero = new Hero();  
    }
}
